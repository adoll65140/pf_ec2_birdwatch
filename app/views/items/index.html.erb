<% @items.each do |item| %>
<%= link_to item_path(item.id) do %>
<div>
  <p>タイトル：<%= item.item %></p>
  <%= attachment_image_tag item, :image, :fill, 60, 60, fallback: "no_image.jpg" %>
  <p>ジャンル：<%= item.genre.name %></p>
  <p>取引場所：<%= item.address_city %><%= item.prefecture_name %></p>
</div>
<% end %>
<% end %>